strPath = "G:\C\" & Year(Now) & Format(Date, "mm") & "\GMI\" & "FUT_RPT_CTEAM_GMI_" & Year(Now) & Format(Date, "mm") & Format(currentDay, "dd") & "\XC115\"

If Dir(strPath) = "" Then
    Exit Sub
    
ElseIf Dir(strPath) <> "" Then
    With MailOutLook 'apply to each new email window
      .To = TOnames
      .CC = CCnames
      .Subject = "Futures Report" & " " & Format(currentDay, "dd") & "/" & Format(Date, "mm") & "/" & Year(Now)
      '~~> *.* for all files
      strFile = Dir(strPath & "*.xls*")
      'loop that directory
      Do While Len(strFile) > 0
      .Attachments.Add strPath + strFile
      strFile = Dir
      Loop
       .Send
    End With
End If
'message box when the loop finishes
'MsgBox "Done!", vbOKOnly
End Sub
Sub SendFuturesReport704()
Dim currentDay As Date
If Weekday(Now(), vbMonday) = 1 Then
    currentDay = Now() - 2
    
ElseIf Weekday(Now(), vbMonday) = 6 Or Weekday(Now(), vbMonday) = 7 Then
    currentDay = ""
    
Else
    currentDay = Now() - 1
    
End If
Dim strFile As String
Dim strPath1 As String
Dim strPath2 As String
Dim CCnames As String
Dim TOnames As String
Dim appOutLook As Outlook.Application
Dim MailOutLook As Outlook.MailItem
Set appOutLook = CreateObject("Outlook.Application")
Set MailOutLook = appOutLook.CreateItem(olMailItem)
CCnames = "fannie.hui@swhyhk.com; kishi.cheng@swhyhk.com"
TOnames = "wendy.kwan@swhyhk.com"
'define where we look for our folder full of xls files
strPath1 = "G:\C\" & Year(Now) & Format(Date, "mm") & "\GMI\" & "FUT_RPT_CTEAM_GMI_" & Year(Now) & Format(Date, "mm") & Format(currentDay, "dd") & "\XC704\"
strPath2 = "G:\C\" & Year(Now) & Format(Date, "mm") & "\GMI\" & "FUT_RPT_CTEAM_GMI_" & Year(Now) & Format(Date, "mm") & Format(currentDay, "dd") & "\C0704\"

If Dir(strPath1) = "" And Dir(strPath2) = "" Then
    Exit Sub
    
ElseIf Dir(strPath1) <> "" And Dir(strPath2) = "" Then
    With MailOutLook 'apply to each new email window
      .To = TOnames
      .CC = CCnames
      .Subject = "Futures Report" & " " & Format(currentDay, "dd") & "/" & Format(Date, "mm") & "/" & Year(Now)
      '~~> *.* for all files
      strFile = Dir(strPath1 & "*.xls*")
      'loop that directory
      Do While Len(strFile) > 0
      .Attachments.Add strPath1 + strFile
      strFile = Dir
      Loop
       .Send
    End With

ElseIf Dir(strPath1) = "" And Dir(strPath2) <> "" Then
    With MailOutLook 'apply to each new email window
      .To = TOnames
      .CC = CCnames
      .Subject = "Futures Report" & " " & Format(currentDay, "dd") & "/" & Format(Date, "mm") & "/" & Year(Now)
      '~~> *.* for all files
      strFile = Dir(strPath2 & "*.xls*")
      'loop that directory
      Do While Len(strFile) > 0
      .Attachments.Add strPath2 + strFile
      strFile = Dir
      Loop
       .Send
    End With
ElseIf Dir(strPath1) <> "" And Dir(strPath2) <> "" Then

    With MailOutLook 'apply to each new email window
      .To = TOnames
      .CC = CCnames
      .Subject = "Futures Report" & " " & Format(currentDay, "dd") & "/" & Format(Date, "mm") & "/" & Year(Now)
      '~~> *.* for all files
      strFile = Dir(strPath1 & "*.xls*")
      'loop that directory
      Do While Len(strFile) > 0
      .Attachments.Add strPath1 + strFile
      strFile = Dir
      Loop
       .Display
    End With
    
    With MailOutLook 'apply to each new email window
      .To = TOnames
      .CC = CCnames
      .Subject = "Futures Report" & " " & Format(currentDay, "dd") & "/" & Format(Date, "mm") & "/" & Year(Now)
      '~~> *.* for all files
      strFile = Dir(strPath2 & "*.xls*")
      'loop that directory
      Do While Len(strFile) > 0
      .Attachments.Add strPath2 + strFile
      strFile = Dir
      Loop
       .Send
    End With
End If

End Sub


